@using DashTechCRM.Models.User
@{ UserObject user = new UserObject();
    if (Session["userInfo"] != null)
    {
        user = Session["userInfo"] as UserObject;
    } }
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <title>DashTechCRM-@ViewBag.title</title>
    <meta content="Admin Dashboard" name="description" />
    <meta content="Themesbrand" name="author" />
    <link rel="shortcut icon" href="/Content/assets/images/favicon.ico">

    <link rel="stylesheet" href="/Content/plugins/morris/morris.css">

    <link href="/Content/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="/Content/assets/css/icons.css" rel="stylesheet" type="text/css">
    <link href="/Content/assets/css/style.css" rel="stylesheet" type="text/css">
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.5.1.min.js"></script>

    <link href="/content/plugins/select2/css/select2.min.css" rel="stylesheet" type="text/css" />

    <script src="/content/plugins/input-mask/jquery.inputmask.js"></script>
    <script src="/content/plugins/input-mask/jquery.inputmask.date.extensions.js"></script>
    <script src="/content/plugins/input-mask/jquery.inputmask.extensions.js"></script>
    <script src="/content/plugins/select2/js/select2.min.js"></script>


    <script src="https://cdnjs.cloudflare.com/ajax/libs/exceljs/3.3.1/exceljs.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/1.3.8/FileSaver.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-polyfill/7.4.0/polyfill.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.5/jszip.min.js"></script>



    <link href="/Content/plugins/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/plugins/datatables/buttons.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <!-- Responsive datatable examples -->
    <link href="/Content/plugins/datatables/responsive.bootstrap4.min.css" rel="stylesheet" type="text/css" />

    <!-- Required datatable js -->
    <script src="/Content/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="/Content/plugins/datatables/dataTables.bootstrap4.min.js"></script>
    <!-- Buttons examples -->
    <script src="/Content/plugins/datatables/dataTables.buttons.min.js"></script>
    <script src="/Content/plugins/datatables/buttons.bootstrap4.min.js"></script>
    <script src="/Content/plugins/datatables/jszip.min.js"></script>
    <script src="/Content/plugins/datatables/pdfmake.min.js"></script>
    <script src="/Content/plugins/datatables/vfs_fonts.js"></script>
    <script src="/Content/plugins/datatables/buttons.html5.min.js"></script>
    <script src="/Content/plugins/datatables/buttons.print.min.js"></script>
    <script src="/Content/plugins/datatables/buttons.colVis.min.js"></script>
    <!-- Responsive examples -->
    <script src="/Content/plugins/datatables/dataTables.responsive.min.js"></script>
    <script src="/Content/plugins/datatables/responsive.bootstrap4.min.js"></script>

    <!-- Datatable init js -->
    <script src="/Content/assets/pages/datatables.init.js"></script>
    <script src="~/Content/plugins/parsleyjs/parsley.min.js"></script>

    <link href="/content/plugins/select2/css/select2.min.css" rel="stylesheet" type="text/css" />

    <script src="/content/plugins/input-mask/jquery.inputmask.js"></script>
    <script src="/content/plugins/input-mask/jquery.inputmask.date.extensions.js"></script>
    <script src="/content/plugins/input-mask/jquery.inputmask.extensions.js"></script>
    <script src="/content/plugins/select2/js/select2.min.js"></script>

    <!-- DevExtreme themes -->
    <link rel="stylesheet" href="https://cdn3.devexpress.com/jslib/20.2.5/css/dx.common.css">
    <link rel="stylesheet" href="https://cdn3.devexpress.com/jslib/20.2.5/css/dx.light.css">

    <!-- DevExtreme library -->
    <script type="text/javascript" src="https://cdn3.devexpress.com/jslib/20.2.5/js/dx.all.js"></script>
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-ui.js"></script>
    <link href="~/Content/Site.css" rel="stylesheet" />
</head>

<body>

    <!-- Navigation Bar-->
    <header id="topnav">
        <div class="topbar-main">
            <div class="container-fluid">

                @Html.Partial("_logoContainer")

                @Html.Partial("_topUserContainer")

                <div class="clearfix"></div>

            </div> <!-- end container -->
        </div>
        <!-- end topbar-main -->
        <!-- MENU Start -->
        <div class="navbar-custom">
            <div class="container-fluid">
                <div id="navigation">
                    <!-- Navigation Menu-->

                    <ul class="navigation-menu">
                        <li class="">
                            <a href="@user.DefUrl"><i class=""></i>Hi, @user.RocketName</a>
                        </li>
                        <li class="has-submenu">

                            <a href="@user.DefUrl"><i class="mdi mdi-home"></i>Dashboard</a>
                        </li>

                        @Html.Partial("_MyCustomMenu")
                        @*@Html.Partial("_menu")*@
                    </ul>
                    <!-- End navigation menu -->
                </div> <!-- end #navigation -->
            </div> <!-- end container -->
        </div> <!-- end navbar-custom -->
    </header>
    <!-- End Navigation Bar-->
    <!-- page wrapper start -->
    <div class="wrapper">
        <div class="page-title-box">
            <div class="container-fluid">
                @Html.Partial("_headerWithTitle")
            </div>
            <!-- end container-fluid -->

        </div>
        <!-- page-title-box -->

        <div class="page-content-wrapper">
            <div class="container-fluid">

                @Html.Partial("_alertBox")
                @RenderBody()
            </div>
            <!-- end container-fluid -->
        </div>
        <!-- end page content-->

    </div>
    <!-- page wrapper end -->
    <!-- Footer -->
    <footer class="footer">
        <div class="container-fluid">

            <div class="row">
                <div class="col-12">
                    2021 © DashTechInc. <span class="d-none d-sm-inline-block"> - CRM Tool <i class="mdi mdi-heart text-danger"></i> by DashTechInc.</span>
                </div>
            </div>
        </div>
    </footer>
    <!-- End Footer -->
    <!-- jQuery  -->
    <script src="/Content/assets/js/bootstrap.bundle.min.js"></script>
    <script src="/Content/assets/js/jquery.slimscroll.js"></script>
    <script src="/Content/assets/js/waves.min.js"></script>

    <script src="/Content/plugins/jquery-sparkline/jquery.sparkline.min.js"></script>

    <!-- Peity JS -->
    <script src="/Content/plugins/peity/jquery.peity.min.js"></script>

    <script src="/Content/plugins/morris/morris.min.js"></script>
    <script src="/Content/plugins/raphael/raphael-min.js"></script>

    <script src="/Content/assets/pages/dashboard.js"></script>

    <!-- App js -->
    <script src="/Content/assets/js/app.js"></script>
    @Scripts.Render("~/bundles/common")
    @*
        @Scripts.Render("~/bundles/dx.all")
        @Styles.Render("~/Content/dxcss")
        @Styles.Render("~/Content/css")*@


</body>

</html>
