            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                    <h4 class="modal-title">Add Follow up Details</h4>
                </div>
                <div class="modal-body">
                    <form action="@Url.Action("ChangeStatus","CandidateTimeLine",new { @area = "common"})" method="get">
                        <input type="hidden" name="cid" id="cid" />
                        <table class="table table-responsive">
                            <tr>
                                <td>Status</td>
                                <td>
                                    @{ var data = CandidateStatusModel.status; }
                                    @*string[] status = { "Sales", "In Marketing", "Technical: In Training", "Marketing:Placed", "Expert: Resume Verification", "Expert: Resume Call Done", "Expert: Not Responding To Resume Team", "Expert: Resume Preparation", "Expert: RUC Pending", "Expert: RUC Done", "Technical: Not Responding For RUC", "Marketing:Not Responding To Recruiter", "Dropped", "Delete Entry", "On Hold", "Account", "Dispute", "Onboarding" };
                                        }*@
                                    <select class="form-control" id="status" name="status">
                                        @foreach (Dictionary<string, string> st in data)
                                        {
                            <option value="@st.Keys">@st.Keys</option>}
                                    </select>
                                </td>
                            </tr>
                            <tr>
                                <td>Message</td>
                                <td>
                                    <textarea class="form-control" id="message" name="message"></textarea>
                                </td>
                            </tr>
                            <tr>
                                <td class="" colspan="2" align="right">
                                    <button class="btn btn-primary">Save</button>
                                </td>
                            </tr>
                        </table>
                    </form>
                </div>
                <div class="modal-footer">
